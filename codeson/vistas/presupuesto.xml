<odoo>
  <data>


	   <!--
	   ##############################
	   Clasificador por objeto del gasto
	   ##############################
	   !-->

	   <record model="ir.ui.view" id="codeson.clas_obj_gasto_form">
         <field name="name">Clasificador por Objeto del gasto</field>
         <field name="model">codeson.clas_obj_gasto</field>
         <field name="arch" type="xml">
            <form>
               <sheet>
                  <h1>
                     <field name="name" />
                  </h1>
		       <group>
			       <group>
				       <field name="partida" />
				       <field name="registro" />
			       </group>
			<group>
                     <field name="utilizar" />
                     <field name="padre"/>
                     </group>
		       </group>

               </sheet>
            </form>
         </field>
      </record>

       <record model="ir.ui.view" id="codeson.clas_obj_gasto_tree">
         <field name="name">Clasificador por Objeto del gasto</field>
         <field name="model">codeson.clas_obj_gasto</field>
         <field name="arch" type="xml">
            <tree>
              <field name="partida"/>
               <field name="name" />


            </tree>


         </field>


      </record>


<record model="ir.actions.act_window" id="codeson.clas_obj_gasto_w">
         <field name="name">Clasificador por objeto del gasto</field>
         <field name="res_model">codeson.clas_obj_gasto</field>
         <field name="view_mode">tree,form</field>
      </record>









	  <menuitem name="Presupuesto" id="codeson.presupuesto_root" sequence="1"/>
	        <menuitem name="Clas. por obj. del gasto" id="codeson.clas_obj_gasto_menu" sequence="2" parent="codeson.presupuesto_root" action="codeson.clas_obj_gasto_w" />
          <menuitem name="Gasto ejercido del presupuesto" id="codeson.gasto_ejercido_presupuesto" sequence="3" parent="codeson.presupuesto_root"/>

  </data>
</odoo>
