<odoo>
<data>
<record id="hr_contract_view_form_cayor" model="ir.ui.view">
   <field name="name">product.template.product.form_codeson</field>
   <field name="model">product.template</field>
   <field name="inherit_id" ref="product.product_template_only_form_view"/>
   <field name="arch" type="xml">
       <data>
           <xpath expr="//page[@name='general_information']" position="after">
               <page string="Objeto del gasto">

                   <group>
                           <field name="de_importacion" attrs="{'invisible': True}"/>
                           <field name="clas1" string="Grupo"
                             attrs="{'invisible': [('de_importacion','=',True)]}"
                             options="{'no_create': True, 'no_create_edit':True}"/>
                           <field name="clas2" string="Subgrupo"
                             attrs="{'readonly':[('clas1', '=', False)],'invisible': [('de_importacion','=',True)]}"
                             options="{'no_create': True, 'no_create_edit':True}"/>
                           <field name="clas3" string="Clase" attrs="{'readonly':[('clas2', '=', False)],'invisible': [('de_importacion','=',True)]}"
                             options="{'no_create': True, 'no_create_edit':True}"/>
                           <field name="partida_especifica" string="Partida especifica" attrs="{'readonly':[('clas3', '=', False)],'invisible': [('de_importacion','=',True)]}"
                             options="{'no_create': True, 'no_create_edit':True}"/>
                           <field name="consecutivo" attrs="{'invisible': True}"/>
                           <field name="codigo" readonly="1"/>
                   </group>
               </page>
           </xpath>
       </data>
   </field>
</record>
</data>
</odoo>
